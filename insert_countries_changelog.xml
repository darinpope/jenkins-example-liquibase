<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd">
    <changeSet id="insert13" author="thien">
        <preConditions onFail="MARK_RAN">
            <tableExists tableName="countries"/>
        </preConditions>     
         <rollback>
        <insert tableName="countries">
            <column name="iso"  value="TT"/>
            <column name="iso3" value="TT"/>
            <column name="iso_numeric" valueNumeric="100"/>
            <column name="country_name"  value="VN"/>
            <column name="capital" value="HANOI"/>
            <column name="continent_code" value="A9"/>
            <column name="currency_code"  value="TTT"/>
        </insert>  
         </rollback>
    </changeSet>
</databaseChangeLog>