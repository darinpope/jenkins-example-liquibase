pipeline {
  agent {
    docker { image 'liquibase/liquibase:latest' }
  }
  stages {
    stage('Status') {
      steps {
        sh '''
        
         liquibase ${ACTION} --changeLogFile=db.changelog-master.xml ${VERSION}
        
        '''
      }
    }
   }
  post {
    always {
      cleanWs()
    }
  }
}
